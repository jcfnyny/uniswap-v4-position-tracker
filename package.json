{
  "name": "uniswap-v4-position-tracker",
  "version": "1.0.0",
  "description": "Backend server for tracking Uniswap V4 liquidity positions across multiple wallets",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typeorm": "typeorm-ts-node-commonjs"
  },
  "keywords": [
    "uniswap",
    "defi",
    "position-tracker",
    "blockchain",
    "ethereum"
